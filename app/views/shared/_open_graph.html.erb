<% @og_props ||= {} %>
<meta property="og:type" content="<%= @og_props[:type] || 'website' %>" />
<meta property="og:url" content="<%= @og_props[:url] || request.url.split('?').first %>" />
<meta property="og:image" content="" />
<meta property="og:site_name" content="Scribble" />
<meta property="og:title" content="<%= @og_props[:title] %>" />
<meta property="og:description" content="<%= @og_props[:description] %>" />
<meta property="fb:app_id" content="" />
